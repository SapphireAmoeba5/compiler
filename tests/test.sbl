func reverse_digits
    dig -> 1
in
    0 dig while dup 0 > do
        swap 10 *
        over 10 % +
        swap 10 /
    end
    swap
end

func print 
    num 
in
    num reverse_digits 
    while dup 0 > do
        dup 10 % 48 + putc
        10 /
    end
end 

