func reverse_digits
    dig -> 1
in
    0 dig while dup 0 > do
        swap 10 *
        over 10 % +
        swap 10 /
    end
    swap
end


func print
    num
in
    num 10 % 48 +
    1 num 10 / while dup 0 > do
        dup 10 % 48 +
        rot rot
        swap 1 + swap
        10 /
    end pop
    
    while dup 0 > do
        swap putc
        1 -
    end
end

func main in 
    0xffffffff print
end